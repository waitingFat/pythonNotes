							var sLeft = (screen.width / 2) - (933 / 2);
							var sTop = (screen.height / 2) - (750 / 2);							
							sURL= 'http://'+sURL+'defaulthmr.asp?popmembers=' + pID + '&isb=0&lng=' + sLNG
							windowOpen(pID, true, sURL, "height=750,width=933,left=" + sLeft + ",top=" + sTop);
						}
						/*LID: 1 ZONE:3*/
					}
					
