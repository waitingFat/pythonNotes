					function tdBC(oE, oT)
					{
						if(oE.id)
						{
							if (Number(oT) == 1)
							{
								oE.className = "lpgb";
							}
							else
							{
								oE.className = "lpyb";
							}
						}
						
						if(oE.parentNode.id)
						{
							var oPE = document.getElementById(oE.parentNode.id);
							if (oPE !== null)
							{
								if (Number(oT) == 1)
